{
  "name": "CYaPass",
  "version": "1.4.4",
  "description": "Draw your password.\nNever type a password again.\nNever memorize a password again.\nNever make up a password again.",
  "main": "js/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": "https://github.com/raddevus/CYaPass-Electron",
  "keywords": [
    "C'YaPass",
    "CYaPass",
    "password",
    "never",
    "type",
    "again"
  ],
  "author": "roger deutsch",
  "license": "CC0-4.0",
  "devDependencies": {
    "@electron-forge/cli": "^6.4.1",
    "@electron-forge/maker-deb": "^6.4.1",
    "@electron-forge/maker-rpm": "^6.4.1",
    "@electron-forge/maker-squirrel": "^6.4.1",
    "@electron-forge/maker-zip": "^6.4.1",
    "@electron-forge/plugin-auto-unpack-natives": "^6.4.1",
    "electron": "^26.1.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "dot-prop": ">=4.2.1",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.5.1",
    "minimist": "^1.2.5"
  },
  "build": {
    "appId": "com.cyapass.CYaPass",
    "copyright": "Copyright Â© 2022 roger deutsch",
    "productName": "CYaPass",
    "electronVersion": "16.0.7",
    "asar": true,
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "linux": {
      "category": "Utility.Passwords",
      "icon": "CYaPass.png",
      "executableName": "cyapass"
    },
    "win": {
      "icon": "build/CYaPass512.png"
    }
  }
}
